  top: "conv5_1"
}
layer {
  name: "conv5_2"
  type: "Convolution"
  bottom: "conv5_1"
  top: "conv5_2"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu5_2"
  type: "ReLU"
  bottom: "conv5_2"
  top: "conv5_2"
}
layer {
  name: "conv5_3"
  type: "Convolution"
  bottom: "conv5_2"
  top: "conv5_3"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0.2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu5_3"
  type: "ReLU"
  bottom: "conv5_3"
  top: "conv5_3"
}
layer {
  name: "roi_pool3"
  type: "ROIPooling"
  bottom: "conv3_3"
  bottom: "rois"
  top: "roi_pool3"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.25 # 1/4
  }
}
layer {
  name: "roi_norm3"
  type: "GRN"
  bottom: "roi_pool3"
  top: "roi_pool3"
}
layer {
  name: "roi_pool4"
  type: "ROIPooling"
  bottom: "conv4_3"
  bottom: "rois"
  top: "roi_pool4"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.125 # 1/8
  }
}
layer {
  name: "roi_norm4"
  type: "GRN"
  bottom: "roi_pool4"
  top: "roi_pool4"
}
layer {
  name: "roi_pool5"
  type: "ROIPooling"
  bottom: "conv5_3"
  bottom: "rois"
  top: "roi_pool5"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.0625 # 1/16
  }
}
layer {
  name: "roi_norm5"
  type: "GRN"
  bottom: "roi_pool5"
  top: "roi_pool5"
}
layer {
  name: "concat5"
  type: "Concat"
  bottom: "roi_pool3" 
  bottom: "roi_pool4"
  bottom: "roi_pool5"
  top: "cat5"
  concat_param {
    axis: 1 # along channels
  }
}
layer {
  name: "scale5"
  type: "Power"
  bottom: "cat5"
  top: "cat5"
  power_param {
    power: 1
    scale: 1000
    shift: 0
  }
}
layer {
  name: "conv_pool5"
  type: "Convolution"
  bottom: "cat5"
  top: "pool5"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    kernel_size: 1
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu_pool"
  type: "ReLU"
  bottom: "pool5"
  top: "pool5"
}
layer {
  name: "int6"
  type: "InnerProduct"
  bottom: "pool5"
  top: "int6"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
    weight_filler {
      type: "gaussian"
      std: 0.0001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "int6"
  top: "int6"
}
layer {
  name: "drop6"
  type: "Dropout"
  bottom: "int6"
  top: "int6"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "int7_1"
  type: "InnerProduct"
  bottom: "int6"
  top: "int7_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "gaussian"
      std: 0.0001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu7_1"
  type: "ReLU"
  bottom: "int7_1"
  top: "int7_1"
}
layer {
  name: "drop7_1"
  type: "Dropout"
  bottom: "int7_1"
  top: "int7_1"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "int7_2"
  type: "InnerProduct"
  bottom: "int6"
  top: "int7_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu7_2"
  type: "ReLU"
  bottom: "int7_2"
  top: "int7_2"
}
layer {
  name: "drop7_2"
  type: "Dropout"
  bottom: "int7_2"
  top: "int7_2"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "adj_score"
  type: "InnerProduct"
  bottom: "int7_1"
  top: "adj_score"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 11
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "adj_bbox"
  type: "InnerProduct"
  bottom: "int7_1"
  top: "adj_bbox"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 44
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "zoom_score"
  type: "InnerProduct"
  bottom: "int7_2"
  top: "zoom_score"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "loss_adj_score"
  type: "SigmoidCrossEntropyLoss"
  bottom: "adj_score"
  bottom: "adj_labels"
  top: "loss_adj_score"
  loss_weight: 1
}
layer {
  name: "loss_adj_bbox"
  type: "SmoothL1Loss"
  bottom: "adj_bbox"
  bottom: "adj_targets"
  bottom: "adj_loss_weights"
  top: "loss_adj_bbox"
  loss_weight: 1
}
layer {
  name: "loss_zoom"
  type: "SigmoidCrossEntropyLoss"
  bottom: "zoom_score"
  bottom: "zoom_labels"
  top: "loss_zoom"
  loss_weight: 1
}

