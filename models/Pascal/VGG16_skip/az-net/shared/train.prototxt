  top: 'rois'
  top: 'adj_labels'
  top: 'adj_targets'
  top: 'adj_loss_weights'
  top: 'zoom_labels'
  python_param {
    module: 'az_data_layer.layer'
    layer: 'AZDataLayer'
    param_str: "'num_classes': 11"
  }
}
layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data"
  top: "conv1_1"
